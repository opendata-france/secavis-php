{
    "name": "opendata-france/secavis-php",
    "type": "library",
    "description": "Service de vérification d'avis d'impôts sur le revenu",
    "keywords": [
        "secavis",
        "php"
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "symfony/css-selector": "^6.2",
        "symfony/dom-crawler": "^6.2",
        "symfony/mime": "^6.2",
        "symfony/http-client": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Secavis\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Secavis\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Adrien Rosi",
            "email": "adrienrosi@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
